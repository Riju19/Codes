#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    long long n,m;
	    
	    cin>>n;
	    m=n-1;
	    if(n==0)
	    {
	        cout<<"NO\n";
	        continue;
	    }
	    int a = n&m;
	    if(a ==0)
	        cout<<"YES";
	   else
	    cout<<"NO";
	    cout<<endl;
	}
	return 0;
}
