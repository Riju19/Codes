#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	
	while(t--)
	{
	    int m,n;
	    cin>>m>>n;
	    
	    int a[m][n];
	    
	    for(int i=0;i<m;i++)
	    {
	        for(int j=0;j<n;j++)
	        {
	            cin>>a[i][j];
	        }
	    }
	    
	    int x;
	    cin>>x;
	    int i=0;
	    int j=n-1;
	    while(i<m && j<n)
	    {
	        if(a[i][j]==x)
	        {
	            cout<<"1\n";
	            break;
	        }
	        if(a[i][j]<x)
	        {
	            if(i!=m-1)
	                i++;
	            else
	            {
	                cout<<"0\n";
	                break;
	            }
	        }
	        else
	        {
	            if(j!=0)
	                j--;
	            else
	            {
	                cout<<"0\n";
	                break;
	            }
	        }
	    }
	    
	}
	return 0;
}
